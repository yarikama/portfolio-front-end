{
  "rewrites": [
    { "source": "/((?!favicon\\.svg$|assets/).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/favicon.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
